bool Neighbour::operator==(const Neighbour& other) {
	return macAddress == other.macAddress;
}

bool Neighbour::operator!=(const Neighbour& other) {
	return !(*this == other);
}

int Neighbour::getMacAddress() const {
	return macAddress;
}

